@model IEnumerable<Web_Store.Models.Employee>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Первое приложение @ViewBag.Title</title>
</head>
<body>
    <h3>Сотрудники</h3>
    <hr/>
    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
            </tr>
        </thead>
        <tbody>
          
            @foreach(var employee in Model)
            {
                <tr>
                    <th>@employee.Id</th>
                    <th>@employee.FirstName</th>
                    <th>@employee.</th>
                    <th></th>
                    <th></th>
                    <th></th>
                </tr>
            }
        </tbody>
       
    </table>
</body>
</html>